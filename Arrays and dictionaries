//dictionaries
var studentsGrade : [String:Int] = ["Ardeshir" : 88 , "Dave" : 76, "john" : 56,"Khashayar" : 100 , "Sam" : 93]
//Strings are keys and the Integers are Values
var KhashayarsGrade = studentsGrade["Khashayar"]
studentsGrade["Ardeshir"] = 97
//add another student to student's Grade
studentsGrade["Mike"] = 41
//All Values
var allValues = [Int](studentsGrade.values)
//all Keys
var allKeys = [String](studentsGrade.keys)
print(studentsGrade)


